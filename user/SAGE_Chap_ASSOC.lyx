\begin_inset Formula 
\[
h(y)=\left\{ {\begin{array}{l}
{\frac{sign(y+\lambda_{2})[(|y+\lambda_{2}|+1)^{\lambda_{1}}-1]}{\lambda_{1}(y_{G2})^{(\lambda_{1}-1)}}}\hfill\\
\\
{y_{G2}sign(y+\lambda_{2})\ln(|y+\lambda_{2}|+1)}\hfill
\end{array}}\right.{\begin{array}{l}
{{if}\;\lambda_{1}\ne0,}\hfill\\
\\
{{if}\;\lambda_{1}=0}\hfill
\end{array}}
\]

\end_inset


\end_layout

\begin_layout Standard
\align left
where
\end_layout

\begin_layout Standard
\align left
\begin_inset Formula 
\[
y_{G2}=\left[{\prod\limits _{i=1}^{N}{(|y_{i}+\lambda_{2}|+1)}}\right]^{\frac{1}{N}}
\]

\end_inset


